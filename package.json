{
  "name": "coolcats-service",
  "version": "1.0.0",
  "description": "Service that listens to the ethereum and polygon networks and updates Cool Cats' serverless back-end",
  "main": "dist/app.js",
  "scripts": {
    "clean": "rm -rf dist/",
    "start": "tsc && node dist/app.js",
    "rescan-block": "tsc && node dist/utility/rescan-block.js",
    "build": "tsc",
    "build-beanstalk": "./scripts/build-beanstalk.sh",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "test": "mocha \"./src/**/*.spec.ts\"",
    "docker-build": "docker build -t coolcats/server .",
    "docker-create": "docker create --name coolcats coolcats/server:latest",
    "docker-start": "docker start coolcats",
    "prepare": "husky install || echo husky not installed",
    "force-nonce": "ts-node src/bin/force-nonce-forward.ts",
    "reset-pets": "ts-node src/bin/reset-pets.ts",
    "reset-pets-and-cats": "ts-node src/bin/reset-pets-and-cats.ts",
    "fix-pet": "ts-node src/bin/fix-pet.ts",
    "fix-pet-stage": "ts-node src/bin/fix-pet-stage.ts",
    "sync-database": "ts-node src/bin/sync-database.ts",
    "reset-game-tables": "ts-node src/bin/reset-game-tables.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@alch/alchemy-web3": "^1.1.7",
    "@coolcatsnft/milk-pets": "^0.0.8",
    "@node-redis/graph": "^1.0.0",
    "@nomiclabs/hardhat-ethers": "^2.0.5",
    "@slack/oauth": "^2.3.0",
    "@types/crypto-js": "^4.1.0",
    "@types/lodash": "^4.14.179",
    "@types/mocha": "^9.1.0",
    "@types/node-fetch": "^2.6.1",
    "@types/redis-mock": "^0.17.1",
    "@types/request": "^2.48.8",
    "@types/uuid": "^8.3.4",
    "@types/yargs": "^17.0.9",
    "0xsequence": "^0.0.0-20220427161307",
    "aws-sdk": "^2.1010.0",
    "bignumber": "^1.1.0",
    "bignumber.js": "^9.0.0",
    "bluebird": "^3.7.2",
    "crypto-js": "^4.1.1",
    "eth-revert-reason": "^1.0.3",
    "ethers": "^5.5.4",
    "lodash": "^4.17.21",
    "mocha": "^9.2.2",
    "mysql": "^2.14.1",
    "newrelic": "^8.7.0",
    "node-fetch": "^2.6.7",
    "prompt-sync": "^4.2.0",
    "pusher": "^5.0.0",
    "redis": "^4.0.2",
    "redis-mock": "^0.56.3",
    "reflect-metadata": "^0.1.10",
    "request": "^2.88.2",
    "sqs-consumer": "^5.6.0",
    "tmp": "^0.2.1",
    "ts-node": "^10.7.0",
    "typeorm": "0.2.38",
    "typescript": "^4.6.2",
    "upgrade": "^1.1.0",
    "uuid": "^8.3.2",
    "web3": "^1.3.0-rc.0",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/newrelic": "^7.0.3",
    "@types/node": "^17.0.24",
    "chai": "^4.3.6",
    "tslint": "^6.1.3"
  },
  "engines": {
    "node": ">=17.5.0"
  }
}
